{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "lint": "eslint src --max-warnings=0",
    "storybook": "start-storybook -s public -p 6006",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@apollo/client": "^3.4.1",
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.3",
    "@ckeditor/ckeditor5-build-classic": "^28.0.0",
    "@ckeditor/ckeditor5-react": "^3.0.2",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "apexcharts": "^3.27.1",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.1",
    "next": "10.2.3",
    "polished": "^4.1.3",
    "prettysize": "^2.0.0",
    "react": "17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.2",
    "react-icons": "^4.2.0",
    "react-kawaii": "^0.17.0",
    "react-player": "^2.9.0",
    "styled-components": "^5.3.0",
    "styled-media-query": "^2.1.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-react-jsx": "^7.14.3",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-kawaii": "^0.11.0",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "apollo": "^2.33.4",
    "babel-loader": "^8.2.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "plop": "^2.7.4",
    "prettier": "2.3.0",
    "storybook-react-context": "^0.2.5",
    "typescript": "^4.3.2"
  }
}
